#!/bin/bash

rm -f prf*.tsv
i=0
while read q ; do
  i=$((i + 1));
  prfout=prf-$i.tsv;
  curl "http://localhost:25806/prf?query=$q&ranker=ql&numdocs=100&numterms=50" > $prfout;
  echo $q:$prfout >> prf.tsv
done < ./data/queries.tsv

java -cp ./bin/classes:./bin/lib/* edu.nyu.cs.cs2580.Bhattacharyya prf.tsv qsim.tsv
